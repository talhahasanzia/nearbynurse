# Backend Environment Variables
# Copy this file to .env and fill in your values

DATABASE_URL=postgresql://postgres:password@db:5432/mydb
PORT=3000
NODE_ENV=development

# Keycloak issuer (e.g. https://auth.myapp.com/realms/yourrealm or http://localhost:8080/realms/master)
KEYCLOAK_ISSUER=http://localhost:8080/realms/master

# Keycloak client ID for password grant
KEYCLOAK_CLIENT_ID=nearbynurse-frontend

# Keycloak admin credentials (for user registration via Admin API)
KEYCLOAK_ADMIN_USERNAME=admin
KEYCLOAK_ADMIN_PASSWORD=admin
